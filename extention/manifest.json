{
  "manifest_version": 2,
  "name": "olxo",
  "description": "My first extention",
  "version": "1.0.0",
  "icons": {"128": "images/e-commerce-icon-icon.png"},
  "background": {
    "scripts": [
      "js/jquery-3.5.1.min.js",
      "js/background.js"
    ],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/site.css"],
      "js": [
        "js/jquery-3.5.1.min.js",
        "js/content.js"
      ],
      "all_frames": true
    }
  ],

  "browser_action": {
    "default_icon": "images/e-commerce-icon-icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "storage",
    "<all_urls>",
    "tabs"
  ],
  "web_accessible_resources": [
    "iframe1.html",
    "iframe2.html",
    "js/iframe.js"
  ]

  }

